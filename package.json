{
  "name": "restaurant-server",
  "version": "1.0.0",
  "description": "A modern restaurant management server with authentication and API endpoints",
  "private": true,
  "main": "app.js",
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "populate": "node populateDb.js",
    "deploy": "node scripts/deploy.js",
    "deploy:prod": "NODE_ENV=production node scripts/deploy.js",
    "deploy:with-data": "node scripts/deploy.js --populate",
    "server": "node server.js",
    "server:prod": "NODE_ENV=production node server.js",
    "db:backup": "node scripts/db-manager.js backup",
    "db:restore": "node scripts/db-manager.js restore",
    "db:clear": "node scripts/db-manager.js clear",
    "db:stats": "node scripts/db-manager.js stats",
    "db:migrate": "node scripts/db-manager.js migrate",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "restaurant",
    "api",
    "express",
    "mongodb",
    "authentication"
  ],
  "author": "Restaurant Team",
  "license": "MIT",
  "dependencies": {
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "debug": "4.3.4",
    "dotenv": "^16.3.1",
    "express": "4.18.2",
    "express-session": "1.17.3",
    "http-errors": "2.0.0",
    "jade": "1.11.0",
    "jsonwebtoken": "9.0.2",
    "mongoose": "6.10.0",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "passport": "0.6.0",
    "passport-facebook-token": "3.3.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "passport-local-mongoose": "7.1.2",
    "session-file-store": "1.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
